source ~/.screenrc

screen -t q
stuff "cd ~/Scripts/q\n"
screen -t python
stuff "cd ~/Scripts/python\n"
screen -t C
stuff "cd ~/Scripts/krc\n"
screen -t julia
stuff "cd ~/Scripts/julia\n"
screen -t pony
stuff "cd ~/Scripts/pony\n"

#set hardstatus
hardstatus alwayslastline "%{db} %= %{W} %w %? %? %="
